svgSocialIcon : String -> List String -> Html msg
svgSocialIcon vBox paths =
    div
        [ class "tw h-6 w-6" ]
        [ svg
            -- [ SvgAttr.Attr.css []
            [ SvgAttr.fill "currentColor"
            , SvgAttr.viewBox vBox
            , Aria.ariaHidden True
            ]
            (List.map
                (\superD ->
                    Svg.path
                        [ SvgAttr.fillRule "evenodd"
                        , SvgAttr.d superD
                        , SvgAttr.clipRule "evenodd"
                        ]
                        []
                )
                paths
            )
        ]


